Character.create(
  name: "Hikoichi",
  name_japanese: "ヒコイチ",
  birthday: "01 de maio",
  description: "É do Esquadrão dos Super Patos de Bicos Manchados que se reportam diretamente à Família Real com Carue como Capitão. É também um meio de transporte para a Realeza. Chopper montou nele à caminho do Going Merry.",
)
Character.create(
  name: "Hiluluk",
